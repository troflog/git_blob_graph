// Eg er stor
digraph trond {
	bgcolor="#f7edd2ed" rankdir=LR
	subgraph cluster_commit {
		fillcolor="#c7c7c7" label=commits style=filled
		C1 [label="Commit 1"]
		C2 [label="Commit 2"]
	}
	subgraph cluster_tree {
		node [shape=box]
		fillcolor="#adada1" label=trees style=filled
		T1 [label=Tree1]
		T2 [label=<<TABLE>
 <TR>
   <TD>left</TD>
   <TD>right</TD>
 </TR>
</TABLE>>]
	}
	subgraph cluster_blob {
		node [shape=box]
		fillcolor="#cfdbc3" label=blobs style=filled
		B1 [label=Blob1]
		B2 [label=Blob2]
	}
	C1 -> C2
	C1 -> T1 [color="#a37575"]
	C2 -> T2 [color=green]
	T1 -> B1 [color=green]
	T2 -> B1 [color=blue]
	T2 -> B2 [color=blue]
}
